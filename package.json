    "gulp-plumber": "^1.1.0",
    "gulp-rimraf": "^0.2.1",
    "gulp-util": "^3.0.8",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-dup-key-validator": "^1.0.2",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "map-stream": "0.0.7",
    "protractor": "~5.4.0",
    "run-sequence": "^1.2.2",
    "through2": "^2.0.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
