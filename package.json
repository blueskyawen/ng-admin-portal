{
  "name": "cephportal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run i18n.merge",
    "start": "npm run i18n.watch & npm run i18n.check & npm run serve-config",
    "start_mock": "npm run start & npm run mock",
    "serve-config": "node --max_old_space_size=6144 ./node_modules/.bin/ng serve --port=5000 --host=0.0.0.0 --disable-host-check --base-href=/cephportal/ --deploy-url=/cephportal/ --open --poll=5000",
    "prebuild": "npm run i18n.merge && npm run i18n.check & npm run lint",
    "build": "node --max_old_space_size=6144 ./node_modules/.bin/ng build --prod --aot --base-href=/cephportal/ --deploy-url=/cephportal/",
    "i18n.check": "gulp i18n-json-key-dup-check",
    "i18n.watch": "gulp watch-i18n-json-file",
    "i18n.merge": "gulp merge-i18n-en-json & gulp merge-i18n-zh-json",
    "mock": "node ./backend/bin/www",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "core-js": "^2.5.4",
    "echarts": "^3.8.5",
    "jquery": "^3.5.1",
    "ng-zorro-antd": "^1.8.1",
    "ngx-echarts": "^3.0.0",
    "rxjs": "~6.2.0",
    "underscore": "^1.8.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.9",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
